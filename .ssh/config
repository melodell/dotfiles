# Personal Github Account
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    UseKeychain yes
    AddKeysToAgent yes

# UMich Gitlab Account
Host gitlab.eecs.umich.edu
    HostName gitlab.eecs.umich.edu
    User bgit
    IdentityFile ~/.ssh/id_rsa_umich
    UseKeychain yes
    AddKeysToAgent yes

# Gitlab Account
Host gitlab.com
    HostName gitlab.com
    IdentityFile ~/.ssh/id_rsa
    UseKeychain yes
    AddKeysToAgent yes

# SSH multiplexing
Host *
  ControlMaster auto
  ControlPersist yes
  ControlPath ~/.ssh/socket-%C
  ServerAliveInterval 60
  ServerAliveCountMax 5
    