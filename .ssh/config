Host github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519

# SSH multiplexing
#
# To avoid long file names when connecting to AWS: ControlPath ~/.ssh/socket-%C
Host *
  ControlMaster auto
  ControlPersist yes
  ControlPath ~/.ssh/socket-%C
  ServerAliveInterval 60
  ServerAliveCountMax 5
